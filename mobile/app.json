{
  "expo": {
    "name": "Raksha Ireland",
    "slug": "raksha-ireland",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "raksha-ireland",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rakshaireland.emergency",
      "buildNumber": "1",
      "developmentTeam": "LZ952SM8Q9",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app needs location access to send emergency alerts to nearby users.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app needs location access to send emergency alerts to nearby users.",
        "UIBackgroundModes": [
          "location",
          "background-processing"
        ]
      }
    },
    "android": {
      "package": "com.rakshaireland.emergency",
      "versionCode": 1,
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "VIBRATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
    },
    "plugins": [
      "expo-location",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": false
          },
          "android": {
            "newArchEnabled": false
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "97e6f9d1-e2a8-4a35-8d65-292212586565"
      }
    },
    "owner": "anshukr"
  }
}
